SELECT a.name AS name,
  round(b.${columnName}::numeric,3) as average_value,
  (SELECT round(AVG(${columnName})::numeric,3) as nat_average_value FROM ${tableName} WHERE year=b.year GROUP BY year)
FROM bra_poladm2 a JOIN ${tableName} b
  ON a.codgov=b.codgov
WHERE a.codgov=${codgov} AND year=${year}
